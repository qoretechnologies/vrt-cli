{
  "name": "@vrt/vrt",
  "version": "0.1.0",
  "description": "Local-first Storybook visual regression capture CLI",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "vrt": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "author": "Foxhoundn",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "p-limit": "^4.0.0",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.54.0",
    "prettier": "^3.0.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "storybook": "^10.2.0",
    "@storybook/react-vite": "^10.2.0",
    "@chromatic-com/storybook": "^5.0.0",
    "@storybook/addon-vitest": "^10.2.0",
    "@storybook/addon-a11y": "^10.2.0",
    "@storybook/addon-docs": "^10.2.0",
    "@storybook/addon-onboarding": "^10.2.0",
    "eslint-plugin-storybook": "^10.2.0",
    "vitest": "^4.0.17",
    "@vitest/browser-playwright": "^4.0.17",
    "@vitest/coverage-v8": "^4.0.17"
  }
}
